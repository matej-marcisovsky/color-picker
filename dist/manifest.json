{
  "name": "Color Picker",
  "description": "Simple color picker.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Pick a color."
  },
  "permissions": ["activeTab", "notifications", "scripting"],
  "background": {
    "service_worker": "Background.js"
  },
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}